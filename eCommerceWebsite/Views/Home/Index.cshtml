@{
    ViewData["Title"] = "Home Page";
}

<h1 class="display-1">Welcome to Canned Food Emporium</h1>

<div id="carouselExampleCaptions" class="carousel slide" data-bs-ride="carousel">
    <div class="carousel-indicators">
        <button type="button" data-bs-target="#carouselExampleCaptions" data-bs-slide-to="0" class="active" aria-current="true" aria-label="Slide 1"></button>
        <button type="button" data-bs-target="#carouselExampleCaptions" data-bs-slide-to="1" aria-label="Slide 2"></button>
    </div>
    <div class="carousel-inner">
        <div class="carousel-item active">
            <img src="~/css/Images/can1.jpg" class="d-block w-50 img-fluid rounded mx-auto d-block" alt="...">
            <div class="carousel-caption d-none d-md-block">
                <h5>Browse our canned goods!</h5>
                <p>Such as this can of zucchini!!!</p>
            </div>
        </div>
        <div class="carousel-item">
            <img src="~/css/Images/can2.jpg" class="d-block w-50 img-fluid rounded mx-auto d-block" alt="...">
            <div class="carousel-caption d-none d-md-block">
                <h5>Or How About This!?</h5>
                <p>Try a delicious can of leaf spinach!</p>
            </div>
        </div>
    </div>
    <button class="carousel-control-prev" type="button" data-bs-target="#carouselExampleCaptions" data-bs-slide="prev">
        <span class="carousel-control-prev-icon" aria-hidden="true"></span>
        <span class="visually-hidden">Previous</span>
    </button>
    <button class="carousel-control-next" type="button" data-bs-target="#carouselExampleCaptions" data-bs-slide="next">
        <span class="carousel-control-next-icon" aria-hidden="true"></span>
        <span class="visually-hidden">Next</span>
    </button>
</div>

<div class="card" style="width: 25rem;">
<img src="~/css/Images/can4.jpg" class="card-img-top d-block w-25 img-fluid" alt="...">
  <div class="card-body">
    <h5 class="card-title">Endless Choices</h5>
    <p class="card-text">We have plenty of canned foods to choose from.</p>
  </div>
  </div>

<div class="card" style="width: 25rem;">
  <img src="~/css/Images/can5.jpg" class="card-img-top d-block w-25 img-fluid" alt="...">
  <div class="card-body">
    <h5 class="card-title">Satisfaction Guaranteed</h5>
    <p class="card-text">We guarantee you'll find something you like!</p>
  </div>
  </div>

<nav aria-label="breadcrumb">
  <ol class="breadcrumb">
    <li class="breadcrumb-item active" aria-current="page">Home</li>
  </ol>
</nav>